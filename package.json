{
  "name": "@jordilo/latevacarta",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start-prod": "ng serve --host 0.0.0.0 --prod=true",
    "build": "npm run test && ng build --prod=true",
    "dev-build": "npm run test && ng build --prod=false -c dev",
    "test": "ng test --collectCoverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format --format-indentation ' ' json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{template,en,ca,es}.json --sort --format json"
  },
  "private": true,
  "dependencies": {
